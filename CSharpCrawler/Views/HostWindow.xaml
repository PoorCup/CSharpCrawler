<Window x:Class="CSharpCrawler.Views.HostWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSharpCrawler.Views"
        mc:Ignorable="d"
        Title="HostWindow" Height="450" Width="800" WindowStyle="None" ResizeMode="NoResize">
    <!--宿主窗体，用于UI美化-->
    <!--并没有什么实际用处 ╮(￣▽￣)╭-->
    <Grid>
        <MediaElement x:Name="mediaelement" Stretch="UniformToFill" Volume="1" MediaEnded="mediaelement_MediaEnded"  LoadedBehavior="Manual" UnloadedBehavior="Manual"/>
    </Grid>
</Window>
